{
  "name": "dash",
  "author": "kalicyh",
  "licence": "MIT",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "concurrently -n 'Frontend,Backend' -c 'blue,green' 'yarn run dev:ui' 'yarn run dev:api'",
    "dev:ui": "vite",
    "dev:api": "poetry run uvicorn api.app:app --host 0.0.0.0 --port 8888 --reload",
    "start": "vite preview",
    "build": "tsc && vite build",
    "dev:host": "vite --host",
    "lib": "yarn install && poetry install --no-root"
  },
  "engines": {
    "node": "20.x"
  },
  "packageManager": "yarn@1.22.22",
  "dependencies": {
    "@emotion/cache": "^11.13.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource-variable/dm-sans": "^5.0.7",
    "@fontsource/barlow": "^5.0.14",
    "@iconify/react": "^5.0.2",
    "@mui/icons-material": "^6.4.4",
    "@mui/lab": "^6.0.0-beta.27",
    "@mui/material": "^6.4.4",
    "apexcharts": "^4.4.0",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "history": "^5.3.0",
    "react": "^19.0.0",
    "react-apexcharts": "^1.4.1",
    "react-dom": "^19.0.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.1.5",
    "simplebar-react": "^3.2.6",
    "sonner": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.5.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "concurrently": "^9.1.2",
    "typescript": "^5.5.4",
    "vite": "^6.1.0",
    "vite-plugin-checker": "^0.8.0"
  }
}
